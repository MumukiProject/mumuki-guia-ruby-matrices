it "Lara tiene las notas correctas" do
  expect(notas_por_alumno[0]). to be [8, 6, 9, 9]
end

it "Miguel tiene las notas correctas" do
  expect(notas_por_alumno[1]). to be [6, 7, 7, 10]
end

it "Micaela tiene las notas correctas" do
  expect(notas_por_alumno[2]). to be [8, 8, 8, 8]
end

it "La matriz tiene las notas de tres alumnos" do
  expect(notas_por_alumno.size). to be 3
end