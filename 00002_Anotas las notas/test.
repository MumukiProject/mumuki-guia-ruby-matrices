it "Lara tiene las notas correctas" do
  expect(notas_por_estudiante[0]). to eq [8, 6, 9, 9]
end

it "Miguel tiene las notas correctas" do
  expect(notas_por_estudiante[1]). to eq [6, 7, 7, 9]
end

it "Micaela tiene las notas correctas" do
  expect(notas_por_estudiante[2]). to eq [8, 8, 8, 8]
end

it "La matriz tiene las notas de tres alumnos" do
  expect(notas_por_estudiante.size). to eq 3
end